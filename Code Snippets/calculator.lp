SET argX := 0
SET argY := 0
SET temp := 0
MOV STDIN -> argX
MOV STDIN -> argY
MOV argY -> temp
ARITH argX + temp
MOV temp -> STDOUT
SET temp := 0
MOV argY -> temp
ARITH argX * temp
MOV temp -> STDOUT
SET temp := 0
MOV argY -> temp
ARITH argX - temp
MOV temp -> STDOUT
SET temp := 0
MOV argY -> temp
ARITH argX / temp 
MOV temp -> STDOUT